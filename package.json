{
  "name": "@hoststack/ui",
  "description": "HostStack's internal component library, built with React, and Stoop.",
  "version": "0.3.4",
  "author": "Jackson Dolman <mail@dolmios.com>",
  "browserslist": {
    "production": [
      "defaults",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://github.com/hoststack/ui/issues"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@googlemaps/js-api-loader": "^2.0.2",
    "@radix-ui/react-icons": "^1.3.2",
    "dayjs": "^1.11.19",
    "fast-sort": "^3.4.1",
    "react-hot-toast": "^2.6.0",
    "react-wrap-balancer": "^1.1.1",
    "stoop": "^0.6.2"
  },
  "devDependencies": {
    "@types/google.maps": "^3.58.1",
    "@types/node": "^25.0.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "bun-types": "^1.3.5",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-dolmios": "^2.0.13",
    "glob": "^13.0.0",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "homepage": "https://github.com/hoststack/ui",
  "keywords": [
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "hoststack",
    "react",
    "react-component",
    "stoop",
    "ui",
    "user interface"
  ],
  "license": "MIT",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "eslint-config-dolmios/configs/prettier",
  "repository": {
    "type": "git",
    "url": "git://github.com/hoststack/ui.git"
  },
  "scripts": {
    "build": "bun run clean && bun run tidy && bun run compile",
    "compile": "bunx tsx scripts/build.ts",
    "clean": "rm -rf dist playground/dist",
    "dev": "bun run compile && bun run playground/dev.ts",
    "kill": "lsof -ti tcp:420 | xargs kill",
    "tidy": "bun run lint && bun run prettier",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "prettier": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "docs": "node scripts/generate-docs.js"
  },
  "sideEffects": false,
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js"
    }
  }
}
